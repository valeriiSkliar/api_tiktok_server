# API Keys
SAD_CAPTCHA_API_KEY=your_sad_captcha_api_key
CHAPTCHA_RESOLVE_MODE=manual

# Headless mode
HEADLESS=true

# Database URLs
DATABASE_POSTGRES_URL=postgresql://user:password@localhost:5432/tiktok
DATABASE_SQLITE_URL=file:./data/tiktok.db
DATABASE_MYSQL_URL=mysql://user:password@localhost:3306/tiktok

# TikTok Credentials
TIKTOK_EMAIL=your_email@example.com
TIKTOK_PASSWORD=your_password

# Crawler Settings
CRAWLER_MAX_WAIT_TIME=30000
CRAWLER_CAPTCHA_TIMEOUT=60000
CRAWLER_MAX_CAPTCHA_ATTEMPTS=3
CRAWLER_HUMAN_DELAY_MIN=500
CRAWLER_HUMAN_DELAY_MAX=2000

# Storage Paths
PATH_SCREENSHOTS=./storage/screenshots
PATH_DATA=./storage/data

# Proxy Settings
PROXY_ENABLED=false
PROXY_URL=http://username:password@proxy.example.com:8080

# User Agent Settings
USER_AGENT_USE_CUSTOM=false
USER_AGENT_CUSTOM=

# Optional Filter Settings
FILTER_REGION=
FILTER_INDUSTRY=
FILTER_OBJECTIVE=
FILTER_PERIOD=
FILTER_AD_LANGUAGE=
FILTER_AD_FORMAT=
FILTER_LIKES=
